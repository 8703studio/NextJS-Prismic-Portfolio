{
  "id": "homepage",
  "label": "Homepage",
  "repeatable": false,
  "status": true,
  "format": "page",
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "uid",
          "placeholder": ""
        }
      },
      "title": {
        "type": "StructuredText",
        "config": {
          "label": "Title",
          "placeholder": "Title of the document",
          "allowTargetBlank": true,
          "single": "heading1"
        }
      },
      "meta_description": {
        "type": "Text",
        "config": {
          "label": "Meta Description",
          "placeholder": "Description used for SEO"
        }
      },
      "meta_title": {
        "type": "Text",
        "config": {
          "label": "Meta title",
          "placeholder": ""
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "marquee": {
              "type": "SharedSlice"
            },
            "hero": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}
